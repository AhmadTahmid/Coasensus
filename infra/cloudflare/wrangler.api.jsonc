{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "coasensus-api",
  "main": "./workers/feed-api/src/index.ts",
  "compatibility_date": "2026-02-19",
  "workers_dev": true,
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "vars": {
    "COASENSUS_APP_ORIGIN": "http://localhost:3000",
    "COASENSUS_DEFAULT_PAGE_SIZE": "20",
    "COASENSUS_AUTO_REFRESH_ON_EMPTY": "1",
    "COASENSUS_INGEST_LIMIT_PER_PAGE": "100",
    "COASENSUS_INGEST_MAX_PAGES": "8",
    "COASENSUS_INGEST_RETRIES": "2",
    "COASENSUS_INGEST_TIMEOUT_MS": "12000",
    "COASENSUS_INGEST_RETRY_BACKOFF_MS": "400",
    "COASENSUS_BOUNCER_MIN_VOLUME": "10000",
    "COASENSUS_BOUNCER_MIN_LIQUIDITY": "5000",
    "COASENSUS_BOUNCER_MIN_HOURS_TO_END": "2",
    "COASENSUS_BOUNCER_MAX_MARKET_AGE_DAYS": "365",
    "COASENSUS_LLM_ENABLED": "0",
    "COASENSUS_LLM_PROVIDER": "openai",
    "COASENSUS_LLM_MODEL": "gpt-4o-mini",
    "COASENSUS_LLM_BASE_URL": "https://api.openai.com/v1",
    "COASENSUS_LLM_PROMPT_VERSION": "v1",
    "COASENSUS_LLM_MIN_NEWS_SCORE": "55",
    "COASENSUS_LLM_MIN_NEWS_SCORE_SPORTS": "72",
    "COASENSUS_LLM_MIN_NEWS_SCORE_ENTERTAINMENT": "78",
    "COASENSUS_LLM_MAX_MARKETS_PER_RUN": "150",
    "COASENSUS_FRONTPAGE_W1": "0.6",
    "COASENSUS_FRONTPAGE_W2": "0.25",
    "COASENSUS_FRONTPAGE_W3": "0.1",
    "COASENSUS_FRONTPAGE_LAMBDA": "0.02",
    "COASENSUS_TOPIC_DEDUP_ENABLED": "1",
    "COASENSUS_TOPIC_DEDUP_SIMILARITY": "0.55",
    "COASENSUS_TOPIC_DEDUP_MIN_SHARED_TOKENS": "5",
    "COASENSUS_TOPIC_DEDUP_MAX_PER_CLUSTER": "1"
  },
  "env": {
    "staging": {
      "name": "coasensus-api-staging",
      "routes": [
        {
          "pattern": "staging.coasensus.com/api/*",
          "zone_name": "coasensus.com"
        }
      ],
      "vars": {
        "COASENSUS_APP_ORIGIN": "https://staging.coasensus.com",
        "COASENSUS_DEFAULT_PAGE_SIZE": "20",
        "COASENSUS_AUTO_REFRESH_ON_EMPTY": "1",
        "COASENSUS_INGEST_LIMIT_PER_PAGE": "100",
        "COASENSUS_INGEST_MAX_PAGES": "8",
        "COASENSUS_INGEST_RETRIES": "2",
        "COASENSUS_INGEST_TIMEOUT_MS": "12000",
        "COASENSUS_INGEST_RETRY_BACKOFF_MS": "400",
        "COASENSUS_BOUNCER_MIN_VOLUME": "10000",
        "COASENSUS_BOUNCER_MIN_LIQUIDITY": "5000",
        "COASENSUS_BOUNCER_MIN_HOURS_TO_END": "2",
        "COASENSUS_BOUNCER_MAX_MARKET_AGE_DAYS": "365",
        "COASENSUS_LLM_ENABLED": "1",
        "COASENSUS_LLM_PROVIDER": "gemini",
        "COASENSUS_LLM_MODEL": "gemini-2.5-flash",
        "COASENSUS_LLM_BASE_URL": "https://generativelanguage.googleapis.com/v1beta",
        "COASENSUS_LLM_PROMPT_VERSION": "v1-gemini-003-billing",
        "COASENSUS_LLM_MIN_NEWS_SCORE": "55",
        "COASENSUS_LLM_MIN_NEWS_SCORE_SPORTS": "72",
        "COASENSUS_LLM_MIN_NEWS_SCORE_ENTERTAINMENT": "78",
        "COASENSUS_LLM_MAX_MARKETS_PER_RUN": "8",
        "COASENSUS_FRONTPAGE_W1": "0.6",
        "COASENSUS_FRONTPAGE_W2": "0.25",
        "COASENSUS_FRONTPAGE_W3": "0.1",
        "COASENSUS_FRONTPAGE_LAMBDA": "0.02",
        "COASENSUS_TOPIC_DEDUP_ENABLED": "1",
        "COASENSUS_TOPIC_DEDUP_SIMILARITY": "0.55",
        "COASENSUS_TOPIC_DEDUP_MIN_SHARED_TOKENS": "5",
        "COASENSUS_TOPIC_DEDUP_MAX_PER_CLUSTER": "1"
      },
      "triggers": {
        "crons": ["*/30 * * * *"]
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "coasensus-staging",
          "database_id": "2ddd58f3-8fa4-472f-b596-6c45d320d923",
          "migrations_dir": "../db/migrations"
        }
      ]
    },
    "production": {
      "name": "coasensus-api",
      "routes": [
        {
          "pattern": "coasensus.com/api/*",
          "zone_name": "coasensus.com"
        }
      ],
      "vars": {
        "COASENSUS_APP_ORIGIN": "https://coasensus.com",
        "COASENSUS_DEFAULT_PAGE_SIZE": "20",
        "COASENSUS_AUTO_REFRESH_ON_EMPTY": "1",
        "COASENSUS_INGEST_LIMIT_PER_PAGE": "100",
        "COASENSUS_INGEST_MAX_PAGES": "8",
        "COASENSUS_INGEST_RETRIES": "2",
        "COASENSUS_INGEST_TIMEOUT_MS": "12000",
        "COASENSUS_INGEST_RETRY_BACKOFF_MS": "400",
        "COASENSUS_BOUNCER_MIN_VOLUME": "10000",
        "COASENSUS_BOUNCER_MIN_LIQUIDITY": "5000",
        "COASENSUS_BOUNCER_MIN_HOURS_TO_END": "2",
        "COASENSUS_BOUNCER_MAX_MARKET_AGE_DAYS": "365",
        "COASENSUS_LLM_ENABLED": "1",
        "COASENSUS_LLM_PROVIDER": "gemini",
        "COASENSUS_LLM_MODEL": "gemini-2.5-flash",
        "COASENSUS_LLM_BASE_URL": "https://generativelanguage.googleapis.com/v1beta",
        "COASENSUS_LLM_PROMPT_VERSION": "v1-gemini-004-prodcheck",
        "COASENSUS_LLM_MIN_NEWS_SCORE": "55",
        "COASENSUS_LLM_MIN_NEWS_SCORE_SPORTS": "72",
        "COASENSUS_LLM_MIN_NEWS_SCORE_ENTERTAINMENT": "78",
        "COASENSUS_LLM_MAX_MARKETS_PER_RUN": "8",
        "COASENSUS_FRONTPAGE_W1": "0.6",
        "COASENSUS_FRONTPAGE_W2": "0.25",
        "COASENSUS_FRONTPAGE_W3": "0.1",
        "COASENSUS_FRONTPAGE_LAMBDA": "0.02",
        "COASENSUS_TOPIC_DEDUP_ENABLED": "1",
        "COASENSUS_TOPIC_DEDUP_SIMILARITY": "0.55",
        "COASENSUS_TOPIC_DEDUP_MIN_SHARED_TOKENS": "5",
        "COASENSUS_TOPIC_DEDUP_MAX_PER_CLUSTER": "1"
      },
      "triggers": {
        "crons": ["*/15 * * * *"]
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "coasensus-production",
          "database_id": "89d18a79-5579-4e8e-9e5c-92389f4bc44a",
          "migrations_dir": "../db/migrations"
        }
      ]
    }
  }
}
